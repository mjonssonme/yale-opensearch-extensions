<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
	<ShortName>Yale Library - Quicksearch</ShortName>
	<LongName>Yale University Library search via Quicksearch Beta</LongName>
	<Description>Search Yale library catalogs and articles with the new Quicksearch interface</Description>
	<InputEncoding>UTF-8</InputEncoding>
	<Image width="16" height="16">data:image/x-icon;base64,</Image>
	<UpdateInterval>7</UpdateInterval>
	<UpdateUrl>http://tripkirkpatrick.commons.yale.edu/wp-content/yale-university-quicksearch.xml</UpdateUrl>
	<IconUpdateUrl>http://www.yale.edu/res/images/favicon.ico</IconUpdateUrl>
	<SearchForm>http://search.library.yale.edu/quicksearch</SearchForm>
	<Url type="text/html" method="GET" template="http://search.library.yale.edu/quicksearch?q={searchTerms}&amp;commit=&amp;sourceid=mozilla-search">
		<Param name="q" value="{searchTerms}" />
		<Param name="commit" value="" />
		<Param name="sourceid" value="mozilla-search" />
	</Url>
	<Url type="application/opensearchdescription+xml" method="GET" template="http://archive.cls.yale.edu/trip/cls/test/yale-university-quicksearch.xml" rel="self">
	</Url>
</SearchPlugin>